{
  "$schema": "https://turbo.build/schema.json",
  "globalDependencies": [".env"],
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": [".svelte-kit/**", ".vercel/**"]
    },
    "dev": {
      "cache": false,
      "persistent": true
    },
    "generate": {
      "cache": false
    },
    "generate:custom": {
      "cache": false
    },
    "db:push": {
      "cache": false
    },
    "train": {
      "cache": false
    }
  }
}