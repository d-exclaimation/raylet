{
  "compilerOptions": {
    "target": "ES2020",
    "module": "NodeNext",
    "lib": ["DOM", "DOM.Iterable", "ES2018.AsyncIterable", "ES2020"],
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "noEmit": true  
  },
  "include": [
    "src/**/*.ts"
  ]
}